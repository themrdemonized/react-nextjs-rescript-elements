{
  "name": "rescript-nextjs-template",
  "namespace": false,
  "jsx": { "version": 4, "mode": "classic" },
  "bs-dependencies": ["@rescript/react", "rescript-classnames"],
  "ppx-flags": [],
  "sources": [
    { "dir": "src", "subdirs": true }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "warnings": {
    "number": "-3",
    "error": "+101+8"
  },
  "gentypeconfig": {
    "language": "untyped",
    "shims": []
  },
  "bsc-flags": ["-open Cx"]
}
